<div class="center hero-unit">
    <h1>
    <% provide(:page_title, 'Push Notification') %>
    <h1>Push Notifications Page</h1>
    <p>
		<%= @hello %>
		<h2>Android Registation ids:</h2>
		<%if @android_reg_ids.any? %>
			<% @android_reg_ids.each do |r| %>
				<%= r %><br/>
			<% end %>
		<% else %>
		<p>no any registration ids found</p>
		<% end %>
		
		<br/>
		
		<h2>Apple Tokens:</h2>
		<%if @apple_reg_ids.any? %>
			<% @apple_reg_ids.each do |r| %>
				<%= r %><br/>
			<% end %>
		<% else %>
		<p>no any tokens found</p>
		<% end %>


    </p>
    <%#= link_to "Sign up now!", signup_path, class: "btn btn-large btn-primary" %>
</div>

<%#= link_to image_tag("rails.png", alt: "Rails"), "http://isit.su" %>
